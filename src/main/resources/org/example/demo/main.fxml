<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="org.example.demo.MainController" spacing="20" alignment="CENTER" prefWidth="600" prefHeight="400">

    <Text text="欢迎来到消消乐小游戏" style="-fx-font-size: 18px; -fx-font-weight: bold;"/>

    <Button fx:id="Button" text="进入游戏" onAction="#handleButton"/>
    <Button fx:id="viewHistoryButton" text="查看排行榜" onAction="#handleViewHistoryAndOnline"/>

</VBox>
