<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="org.example.demo.MainController" spacing="20" alignment="CENTER" prefWidth="600" prefHeight="400">

    <Text text="欢迎使用游戏系统" style="-fx-font-size: 18px; -fx-font-weight: bold;"/>

    <Button fx:id="loginButton" text="登录" onAction="#handleLoginButton"/>
    <Button text="注册" onAction="#handleRegisterButton"/>

</VBox>
